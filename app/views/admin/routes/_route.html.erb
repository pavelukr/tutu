<%= javascript_pack_tag 'application' %>
<tr>
  <td>
    <div id="route_name_<%= route.id %>">
    <%= route.name %>
    </div>
    <%= render 'form', route: route, hidden: true %>
  </td>
  <td><%= link_to 'Show', [:admin, route] %></td>
  <td><%= link_to 'Edit', '', class: "edit_route", data: { route_id: route.id } %></td>
  <td><%= link_to 'Delete', [:admin, route], method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>